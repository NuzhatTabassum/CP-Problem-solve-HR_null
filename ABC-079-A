#include <stdio.h>
 
int main()
{
    int a[4],i=0,n,r;
    scanf("%d",&n);
    while(i<4)
    {
        r=n%10;
        a[i]=r;
        i++;
        n=n/10;
    }
    if(a[1]==a[2] && a[2]==a[3] )
        printf("Yes");
    else if(a[1]==a[2] && a[2]==a[3] && a[3]==a[4])
    printf("Yes");
    else
        printf("No");
  return 0;
}
